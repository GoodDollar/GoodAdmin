    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.5.32",
    "@types/node-fetch": "^2.5.4",
    "@types/pino": "^5.8.13",
    "async-lock": "^1.2.4",
    "bip39-light": "^1.0.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "convict": "^5.2.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gun": "^0.2019.1120",
    "http-status": "^1.4.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "mongoose": "^5.7.13",
    "node-fetch": "^2.6.0",
    "pino": "^5.14.0",
