  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/eslint": "^6.1.3",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "concurrently": "^5.0.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.7.2",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "start": "npm run watch",
    "start:server": "node dist/index.js",
    "start:node": "npm run build && npm run serve",
    "build": "npm run build:ts",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "serve": "env-cmd node dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch:ts\" \"npm run watch:node\"",
    "watch:node": "env-cmd nodemon dist/index.js",
    "watch:ts": "tsc -w",
    "build:ts": "tsc",
    "debug": "npm run build && npm run debug:watch",
    "debug:serve": "env-cmd nodemon --inspect dist/index.js",
    "debug:watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch:ts\" \"npm run debug:serve\""
  },
  "engines": {
    "node": ">=12.0"
  }
}
